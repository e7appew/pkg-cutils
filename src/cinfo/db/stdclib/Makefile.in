srcdir = @srcdir@

prefix = @prefix@

etcdir = @prefix@/share/cutils/cinfo

INSTALL_PROGRAM = @INSTALL_PROGRAM@
INSTALL_DATA = @INSTALL_DATA@
INSTALL = @INSTALL@

DB = stdclib.cinfo

all: $(DB).db

$(DB).db:
	../../cinfoc/cinfoc $(DB)

install: $(DB)
	$(srcdir)/../../../../mkinsdir.sh $(etcdir)
	$(INSTALL_DATA) $(DB).db $(etcdir)

clean:
	-rm -f $(DB).db

distclean: clean 
	-rm -f Makefile

realclean: distclean

mostlyclean: clean
