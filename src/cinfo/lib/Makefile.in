srcdir = @srcdir@

CC = @CC@
CFLAGS = -I../../.. -I$(srcdir)/../../../lib @CFLAGS@
LDFLAGS = @LDFLAGS@
LIBS = @LIBS@
RANLIB = @RANLIB@

LIB = libcinfo.a
OBJS = fileio.o tree.o

.c.o:
	$(CC) -c -I. -I$(srcdir) $(CFLAGS) $<

all: $(LIB)

$(LIB): $(OBJS)
	rm -f $(LIB)
	ar rc $(LIB) $(OBJS)
	$(RANLIB) $(LIB)

install:

clean: 
	-rm -f $(LIB) *.o

distclean: clean 
	-rm -f Makefile

realclean: distclean

mostlyclean: clean
