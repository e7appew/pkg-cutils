#	$Id: Makefile.in,v 1.5 1997/06/15 11:41:51 sandro Exp $

subdirs = cdecl chilight cinfo cobfusc ctangle cundecl cunloop cweave \
	yyextract yyref

all install clean realclean mostlyclean:
	@for f in $(subdirs); do \
		(cd $$f && $(MAKE) $@) \
	done

distclean:
	@for f in $(subdirs); do \
		(cd $$f && $(MAKE) $@) \
	done
	-rm -f Makefile
